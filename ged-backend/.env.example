# AWS Credentials (read automatically by AWS SDK)
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key

# S3 Configuration (use double underscore for nested config)
S3__Region=eu-west-1
S3__BucketName=your-ged-bucket
S3__ServiceUrl=                    # Leave empty for AWS, or http://localhost:4566 for LocalStack
S3__MaxFileSizeMb=100
